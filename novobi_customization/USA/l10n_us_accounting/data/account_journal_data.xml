<?xml version="1.0" encoding="utf-8"?>
<odoo noupdate="1">
    <function id="dedicated_credit_note_sequence_data"
              model="account.journal"
              name="update_sequences">
        <value>Customer Invoices</value> <!-- Name of Account Journal -->
        <value>INV</value> <!-- Code of Account Journal -->
        <value>RINV</value>
        <value>CRN</value>
    </function>

    <function id="dedicated_vendor_credit_note_sequence_data"
              model="account.journal"
              name="update_sequences">
        <value>Vendor Bills</value> <!-- Name of Account Journal -->
        <value>BILL</value> <!-- Code of Account Journal -->
        <value>RBILL</value>
        <value>VCRN</value>
    </function>

    <function id="dedicated_write_off_sequence_data"
              model="account.journal"
              name="create_write_off_sequences">
        <value>Customer Invoices</value> <!-- Name of Account Journal -->
        <value>INV</value> <!-- Code of Account Journal -->
        <value eval="1"/> <!-- Init sequence is 1 -->
        <value>WRO</value>
    </function>

    <function id="auto_allow_cancelling_entries"
              model="account.journal"
              name="auto_allow_cancelling_entries">
    </function>
</odoo>