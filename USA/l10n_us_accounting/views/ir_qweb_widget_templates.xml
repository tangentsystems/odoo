<odoo>
    <template id="contact_name" inherit_id="base.contact_name">
        <div position="after">
            <div t-if="'check_name' in fields">
                <t t-if="object.check_name">
                    <span itemprop="name" t-esc="object.check_name"/>
                </t>
                <t t-else="">
                    <span itemprop="name" t-esc="object.name"/>
                </t>
            </div>
        </div>
    </template>
</odoo>